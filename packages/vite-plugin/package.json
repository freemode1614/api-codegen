{
  "name": "@apicodegen/vite-plugin",
  "version": "0.0.0",
  "description": "Code generator base on typescript AST tools",
  "type": "module",
  "author": "freemode",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsup --watch",
    "build": "cross-env NODE_ENV=production tsup"
  },
  "dependencies": {
    "@apicodegen/openapi": "workspace:*",
    "@moccona/logger": "^0.0.2",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0"
  },
  "browserslist": [
    "modern"
  ]
}
