{
  "name": "@moccona/apicodegen",
  "version": "0.0.2",
  "description": "",
  "type": "module",
  "bin": {
    "apicodegen": "./bin/cli.cjs"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest --watch false",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*/index.js",
      "import": "./npm/*/index.d.ts"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js"
    }
  },
  "keywords": [
    "api-codegen"
  ],
  "author": "freemode",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@moccona/logger": "^0.0.2",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "openapi-types": "^12.1.3",
    "undici": "^7.8.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@moccona/eslint-config": "^1.0.6",
    "@moccona/preset-config": "^0.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.14.1",
    "@types/serve-handler": "^6.1.4",
    "axios": "^1.11.0",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.9",
    "prettier": "^3.6.2",
    "serve": "^14.2.4",
    "serve-handler": "^6.1.6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "prettier": "^3",
    "typescript": "^5",
    "vite": "^7"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "prettier": "@moccona/preset-config/prettierrc",
  "packageManager": "pnpm@10.9.0",
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ]
}
